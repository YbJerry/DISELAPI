<?xml version="1.0" encoding="UTF-8"?>
<ontology>
    <name>weather</name>
    <atomDomain>
        <atom>
            <name>Humidity</name>
            <description>The humidity percent of the day</description>
        </atom>
        <atom>
            <name>MaxTemp</name>
            <description>The maximum temperature of the day</description>
        </atom>
        <atom>
            <name>MinTemp</name>
            <description>The minimum temperature of the day</description>
        </atom>
        <atom>
            <name>Rainfall</name>
            <description>The rainfall index of the day</description>
        </atom>
        <atom>
            <name>Sunshine</name>
            <description>The rainfall index of the day</description>
        </atom>
    </atomDomain>
    <concept>
        <name>Moisture</name>
        <latticeOfConcepts>
            <atom>Humidity</atom>
            <atom>Rainfall</atom>
        </latticeOfConcepts>
    </concept>
    <concept>
        <name>Spring</name>
        <latticeOfConcepts>
            <atom>hemisphere</atom>
            <atom>month</atom>
        </latticeOfConcepts>
    </concept>
    <concept>
        <name>Sunny</name>
        <latticeOfConcepts>
            <atom>Rainfall</atom>
            <atom>Sunshine</atom>
            <atom>maxTemp</atom>
            <atom>minTemp</atom>
        </latticeOfConcepts>
    </concept>
    <concept>
        <name>Temp</name>
        <latticeOfConcepts>
            <atom>maxTemp</atom>
            <atom>minTemp</atom>
        </latticeOfConcepts>
    </concept>
    <concept>
        <name>Windy</name>
        <latticeOfConcepts>
            <atom>Sunshine</atom>
        </latticeOfConcepts>
    </concept>
    <concept>
        <name>weather</name>
        <latticeOfConcepts/>
    </concept>
    <graph>
        <name>Weather Graph</name>
        <rootedIn>weather</rootedIn>
        <edge>
            <from>Seasons</from>
            <to>weather</to>
        </edge>
        <relation>
            <name>isA</name>
            <properties>transitive reflexive </properties>
            <edge>
                <from DIS="dis2">Summer</from>
                <to>Seasons</to>
            </edge>
            <edge>
                <from>Fall</from>
                <to>Seasons</to>
            </edge>
            <edge>
                <from>Winter</from>
                <to>Seasons</to>
            </edge>
            <edge>
                <from>Spring</from>
                <to>Seasons</to>
            </edge>
            <edge>
                <from>Sunny Day</from>
                <to>Summer</to>
            </edge>
            <edge>
                <from>Crisp Day</from>
                <to>Fall</to>
            </edge>
            <edge>
                <from>Frigid Day</from>
                <to>Winter</to>
            </edge>
            <edge>
                <from>Clear Day</from>
                <to>Spring</to>
            </edge>
        </relation>
    </graph>
</ontology>
